<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yofficial">


    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        >
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HyunWooActivity"></activity>
        <activity android:name=".JaeWonActivity"></activity>
        <activity android:name=".JoonHongActivity"></activity>
        <activity android:name=".YouTubeActivity"></activity>
        <activity android:name=".DBActivity"></activity>
        <activity android:name=".VideoListActivity"></activity>
        <activity android:name=".CreateRecipeActivity"></activity>


    </application>

</manifest>